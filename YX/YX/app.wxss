/**app.wxss**/
@import '/vector/asse.wxss';
@import '/vector/style/weui.wxss';

page{
    background-color: #fff;
    font-size: 15px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}

.lineCon{
  display: flex;
  width: 100%;
  border-bottom: 1px solid #eee;
  padding: 5px 0; 
  margin: 5px 0 5px 0;
}
.rowBlue{
  width: 10px;
  border-radius: 10px;
  margin:0 5px;
  background: #79B2F9;
}

.link{
  color: #4C98F7;
}