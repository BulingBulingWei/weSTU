page{
  padding: 10px;
  width: calc(100% - 20px);
}

.sdustMap{
  width: 100%;
  border-radius: 3px;
}

.sdustMap{
  width: 100%;
  height: 300px;
}

cover-view{
  text-align: right;
  font-size: 15px;
  color: #666;
  border-radius: 3px;
}

.tips{
  border:1px solid #eee;
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 5px;
  background: #eee;
  color: #666;
  font-size: 15px;
  border-radius: 3px;
}

.tips view{
  margin-left: 5px;
  align-self: center;
}

.point{
  width: 8px;
  height: 8px;
  border-radius: 8px;
}

.loc{
  line-height: 20px;
}

.ImgFrom{
  text-align: right;
  font-size: 12px;
  color: rgb(122, 122, 122);
  position: absolute;
  bottom: 7px;
  right: 5px;
}

.card{
 
  display: flex;
  border-bottom:1px solid #eee;
  padding: 15px 10px;
  box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.123);
  margin-bottom: 10px;
}

.btn{
  width: 60%;
  margin-top: 20rpx;
  background-color: greenyellow;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.btn::after {
  border-radius: 98rpx;
  border: 0; 
}

/*card内的布局*/
.supple{
  display: flex;
  flex-direction: column;
}

/*text文本居中*/
.center{
  display:flex;
  align-items:center;
  justify-content:center;
}

/*text文字样式*/
.fontStyle{
  font-size: 1rem;
  font-weight: 400;
}